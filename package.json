{
  "name": "mushyak.github.io",
  "version": "1.0.0",
  "description": "FrontCamp 2017",
  "main": "main.js",
  "dependencies": {
    "promise-polyfill": "6.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "buildFetch": "babel node_modules/whatwg-fetch -d dist/libs --presets env",
    "buildPromise": "babel node_modules/promise-polyfill -d dist/libs/promise --presets env",
    "compile": "babel scripts -d dist --presets env",
    "buildLibs": "npm run buildPromise | npm run buildFetch",
    "build": "npm run buildLibs | npm run compile",
    "start": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mushyak/mushyak.github.io.git"
  },
  "author": "Irina Kouznetsova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mushyak/mushyak.github.io/issues"
  },
  "homepage": "https://github.com/mushyak/mushyak.github.io#readme"
}
